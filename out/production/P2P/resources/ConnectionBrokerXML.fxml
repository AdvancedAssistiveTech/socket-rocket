<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.TextFlow?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="core.controllers.BrokerController">
   <children>
      <ProgressBar fx:id="progressBar" prefWidth="300.0" progress="0.0">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin>
      </ProgressBar>
      <TextFlow fx:id="logFlow" prefHeight="200.0" prefWidth="300.0" style="-fx-border-color: #140f3c;">
         <VBox.margin>
            <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
         </VBox.margin>
         <padding>
            <Insets left="5.0" right="5.0" />
         </padding>
      </TextFlow>
      <Button fx:id="btnCancel" mnemonicParsing="false" text="Cancel">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" top="5.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
